<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/ui/css/bootstrap.css">
    <link rel="stylesheet" href="~/ui/js/bootstrap.min.js" />
    @*<link rel="stylesheet" type="text/css" href="~/ui/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/ui/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/ui/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/ui/css/owl.theme.default.css">
    <link rel="stylesheet" href="~/ui/css/style.css" />
    <link rel="stylesheet" href="~/ui/css/bootstrap-social.css">
    <script type="text/javascript" src="~/ui/js/jquery-1.11.0.min.js" crossorigin="anonymous"></script>
</head>

<body class="@ViewBag.BodyClass">
    <header id="header">
        <div class="container">
            <h1>
                <a href="/">
                    Talent's <span>Profile</span><p id="slogan">Join! Let's discover the world.</p>
                </a>
            </h1>
            <nav id="navbar-secondary">
                <ul>
                    <li class="active" id="dashboard"><a href="/adm/dashboard">Dashboard</a></li>
                    <li class="" id="corporate">
                        <a href="/adm/corporate">
                            Corporate
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="#" data-toggle="modal" data-target="#manageProfile">Add Organization</a></li>
                            <li><a href="#" id="viewOrgList">View Organizations</a></li>
                            @*<li><a href="../dashboard/viewprofile">View Profile</a></li>
                                <li><a href="#">Advertisement</a></li>
                                <li><a href="#">Corporate Page</a></li>
                                <li class="divider"></li>
                                <li><a href="#">Search Analytics</a></li>
                                <li><a href="#">Performance</a></li>*@
                        </ul>
                    </li>
                    <li class="" id="search"><a href="/adm/searches">Search</a></li>
                    <li class="" id="jobs">
                        <a href="/adm/jobs">Jobs <b class="caret"></b></a>
                        <ul class="dropdown-menu">
                            <li><a href="../employer/postjob?add=new">Add New Job</a></li>
                            <li><a href="../employer/postjob">All Posted Jobs</a></li>
                            <li class="divider"></li>
                            <li><a href="../employer/walkins?add=new">Walk-in Event</a></li>
                        </ul>
                    </li>
                    <li class="dropdown" id="accounts">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            Accounts<b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="#">All Users</a></li>
                            <li><a href="#">Create User</a></li>
                            <li class="divider"></li>
                            <li><a href="#">Notifications</a></li>
                            <li><a href="#">Analytics</a></li>
                            <li class="divider"></li>
                            <li><a href="#">Settings</a></li>
                            <li><a href="/adm/dashboard/logout">Logout</a></li>
                        </ul>
                    </li>
                    @*<li><a href="#" data-toggle="modal" data-target="#userLogin"><i class="fa fa-lock"></i>&nbsp;Login</a></li>
                        <li>
                            <a href="#"><i class="fa fa-user"></i>&nbsp;Create Account <i class="fa fa-angle-down" aria-hidden="true"></i></a>
                            <ul>
                                <li><a href="/talent/signup"><i class="fa fa-graduation-cap"></i>&nbsp;New Talent</a></li>
                                <li><a href="/employer/register"><i class="fa fa-institution"></i>&nbsp;Recuiter</a></li>
                            </ul>
                        </li>*@
                </ul>
            </nav>
        </div>
        <hr />
    </header>
    <section class="content-wrapper">
        <div class="container">
            <div class="row" style="margin-top:30px">
                <div class="col-md-8">
                    @RenderBody()
                </div>
                <div class="col-md-4">
                    <!-- Right side contents -->
                    @*@Html.Partial("../Dashboard/Components/EmpMenu")*@
                    @Html.Partial("../Components/Highlights")
                </div>
            </div>
        </div>
        <div class="clearfix">&nbsp;</div>
    </section>

    @Html.Partial("../Shared/FooterLinks")

</body>
</html>


<link href="/ui/css/jquery-ui.css" rel="stylesheet" />
<script src="/ui/js/jquery-ui.js" type="text/javascript"></script>
<script src="/ui/js/bootstrap-dropdown.js"></script>
<script src="/ui/js/docs.js"></script>
<script src="/ui/js/bootstrap.js"></script>
<script src="/ui/js/owl.carousel.min.js"></script>
<script src="/ui/js/main.js"></script>
<script src="/ui/adm/ui_controller.js" type="text/javascript"></script>

